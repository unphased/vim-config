# Load this file with dconf (see paperwm-keybindings.apply.sh) to apply
# the PaperWM and GNOME keybinding overrides in one shot.
[org/gnome/shell/extensions/paperwm/keybindings]
switch-left=['<Super>h']
switch-down=['<Super>j']
switch-up=['<Super>k']
switch-right=['<Super>l']

[org/gnome/desktop/wm/keybindings]
hide-window=@as []
